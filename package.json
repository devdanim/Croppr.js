{
  "name": "dnm-croppr",
  "version": "2.3.3",
  "description": "A fork of original Croppr.js, a vanilla JavaScript image cropper, with advanced options.",
  "main": "src/index.js",
  "source": "src/index.js",
  "scripts": {
    "build": "rollup --config rollup.config.dev.js & cp src/css/croppr.css dist/dnm-croppr.css",
    "watch": "onchange \"src/**/*.*\" -v -- npm run build",
    "production": "rollup --config rollup.config.prod.js & cleancss src/css/croppr.css -o dist/dnm-croppr.min.css",
    "dist": "npm run build && npm run production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdanim/dnm-croppr"
  },
  "author": "Adrien du Repaire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devdanim/dnm-croppr/issues"
  },
  "homepage": "https://github.com/devdanim/dnm-croppr#readme",
  "keywords": [
    "image",
    "cropper",
    "cropping",
    "crop",
    "javascript",
    "js",
    "vanilla",
    "native",
    "croppr"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "clean-css": "^3.4.23",
    "onchange": "^3.2.1",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-uglify": "^1.0.1"
  },
  "babel": {
    "presets": [
      "es2015-rollup"
    ]
  }
}
