{
  "name": "dmcroppr",
  "version": "2.3.2",
  "description": "A fork of original Croppr.js, a vanilla JavaScript image cropper, with advanced options.",
  "main": "dist/dmcroppr.js",
  "scripts": {
    "build": "rollup --config rollup.config.dev.js & cp src/css/croppr.css dist/dmcroppr.css",
    "watch": "onchange \"src/**/*.*\" -v -- npm run build",
    "production": "npm run build & rollup --config rollup.config.prod.js & cleancss src/css/croppr.css -o dist/dmcroppr.min.css",
    "dist": "npm run build && npm run production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdanim/DmCroppr.js"
  },
  "author": "Adrien du Repaire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devdanim/DmCroppr.js/issues"
  },
  "homepage": "https://github.com/devdanim/DmCroppr.js#readme",
  "keywords": [
    "image",
    "cropper",
    "cropping",
    "crop",
    "javascript",
    "js",
    "vanilla",
    "native",
    "croppr"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "clean-css": "^3.4.23",
    "imports-loader": "^0.7.0",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.2.0",
    "onchange": "^3.2.1",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "^1.17.7"
  },
  "babel": {
    "presets": [
      "es2015-rollup"
    ]
  }
}
